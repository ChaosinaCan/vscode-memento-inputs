{
	"name": "memento-inputs",
	"displayName": "Memento Inputs",
	"description": "Create task inputs that remember their last-used values",
	"repository": {
		"type": "git",
		"url": "https://github.com/ChaosinaCan/vscode-memento-inputs"
	},
	"author": {
		"name": "Joel Spadin",
		"email": "joelspadin@gmail.com",
		"url": "https://github.com/ChaosinaCan"
	},
	"publisher": "spadin",
	"license": "MIT",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:memento.pickString",
		"onCommand:memento.promptString"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "memento.pickString",
				"title": "Pick String"
			},
			{
				"command": "memento.promptString",
				"title": "Prompt String"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "memento.pickString",
					"when": "false"
				},
				{
					"command": "memento.promptString",
					"when": "false"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vsce": "^1.66.0",
		"vscode-test": "^1.0.2"
	}
}
